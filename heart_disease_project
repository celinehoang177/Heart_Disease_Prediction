      "source": [
        "def age_size(row):\n",
        "    if row[\"age\"] <= 42:\n",
        "        return \"1\"\n",
        "    elif row[\"age\"] <= 60:\n",
        "        return \"2\"\n",
        "    else: #We assume all other values are in the middle, so we assign medium to them\n",
        "        return \"3\"\n",
        "\n",
        "df['age_size'] = df.apply(age_size,axis=1)\n",
        "df[['age','age_size']].head()"
      ],
      "execution_count": null,
      "outputs": [
