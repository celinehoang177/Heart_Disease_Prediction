        "def heart_rate_classify (row):\n",
        "    if row['thalach'] <= 160:\n",
        "        return \"1\"\n",
        "    elif row['thalach'] <= 170:\n",
        "        return \"2\"\n",
        "    else:\n",
        "        return \"3\"\n",
        "df['thalach_group'] = df.apply(heart_rate_classify,axis=1)\n",
        "\n",
        "df[['thalach','thalach_group']].head()"
